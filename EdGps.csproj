      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Voice\Earth.mp3">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Voice\Identified.mp3">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
